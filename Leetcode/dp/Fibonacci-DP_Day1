class Solution {
    public int fib(int n) {
        
        int[] fibonacciSeries = new int[31];
        fibonacciSeries[0]=0;
        fibonacciSeries[1]=1;
        for(int i=2;i<=n;i++){
            fibonacciSeries[i]=fibonacciSeries[i-1]+fibonacciSeries[i-2];
        }
        return fibonacciSeries[n];
        
    }
}
